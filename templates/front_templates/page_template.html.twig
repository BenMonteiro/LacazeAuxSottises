{% extends "front_layout.html.twig" %}

{% block title %}
	{{ ('front.' ~ page.tab)|trans({}, 'menu')}}/{{('front.' ~ page.name)|trans({}, 'menu')}}
{% endblock %}

{% block header %}
	{% include( "front_templates/_front_menu.html.twig") %}
{% endblock %}

{% block pageTitle %}
	<div id="pageTitleBar">
		<div class="d-flex justify-content-center">
			<p class="mt-3 pageTitle px-4">
				{{ ('front.'~ page.name)|trans({}, 'menu')}}
			</p>
		</div>
		<div class="titleLines">
			<img class="titleBar" src="{{asset('images/title-bar.jpg')}}">
		</div>
	</div>
{% endblock %}
