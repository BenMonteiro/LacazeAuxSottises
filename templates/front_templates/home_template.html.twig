{% extends "front_layout.html.twig" %}

{% block title %}
	{{ ('front.' ~ page.tab)|trans({}, 'menu')}}/{{('front.' ~ page.name)|trans({}, 'menu')}}
{% endblock %}

{% block header %}
	{% include( "front_templates/_front_menu.html.twig") %}
{% endblock %}

{% block pageTitle %}
	<div id="pageTitleBar">
		<img class=" d-xl-block d-none homeLeftCircle" src="{{asset('images/biColorCircle.jpg')}}" alt="cercle pointillé rouge et noir">
		<img class=" d-md-block d-none homeRightCircle" src="{{asset('images/redCircle.jpg')}}" alt="cercle pointillé rouge">
		<div class="d-flex justify-content-center">
			<h1 class="mt-3 pageTitle px-4 text-center">
				{{'general.highlight'|trans({}, 'general')|capitalize}}<br>
				{{ 'general.month'|trans({}, 'general')}}
			</h1>
		</div>
		<div class="titleLines">
			<img class="homeTitleBar" src="{{asset('images/title-bar.jpg')}}" alt="barre grise">
		</div>
	</div>
{% endblock %}
