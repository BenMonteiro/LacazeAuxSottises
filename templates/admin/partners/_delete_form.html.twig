<form method="post" action="{{ path('partners_delete', {'id': partner.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_method" value="DELETE">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ partner.id) }}">
	<button class=" badge badge-danger btn my-2 p-2">{{'general.delete'|trans({}, 'general')|capitalize}}</button>
</form>
